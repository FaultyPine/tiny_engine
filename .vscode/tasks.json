{
    "version": "2.0.0",
    "tasks": [
        {
            "taskName": "build",
            "type": "shell",
            "command": "build.bat norun",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": "$gcc"
        },
        {
            "taskName": "build_types",
            "type": "shell",
            "command": "python build.py",
            "group": {
                "kind": "build",
                "isDefault": false
            },
            "problemMatcher": "$gcc",
            "options": {
                "cwd": "${workspaceFolder}\\types"
            }
        },
    ]
}