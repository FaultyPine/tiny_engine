{
    "configurations": [
    {
        "name": "Testbed",
        "type": "cppvsdbg",
        "request": "launch",
        "program": "${workspaceFolder}\\bin\\Testbed.exe",
        "args": ["${workspaceFolder}\\res\\"],
        "stopAtEntry": false,
        //"preLaunchTask": "build_all",
        "cwd": "${workspaceFolder}\\bin",
        "environment": [],
        "console": "integratedTerminal"
    },
    {
        "name": "Movementproj",
        "type": "cppvsdbg",
        "request": "launch",
        "program": "${workspaceFolder}\\bin\\Movement.exe",
        "args": ["${workspaceFolder}\\res\\"],
        "stopAtEntry": false,
        //"preLaunchTask": "build_all",
        "cwd": "${workspaceFolder}\\bin",
        "environment": [],
        "console": "integratedTerminal"
    },
    {
        "name": "Editor",
        "type": "cppvsdbg",
        "request": "launch",
        "program": "${workspaceFolder}\\bin\\TinyEditor.exe",
        "args": ["${workspaceFolder}\\res\\"],
        "stopAtEntry": false,
        //"preLaunchTask": "build_all",
        "cwd": "${workspaceFolder}\\bin",
        "environment": [],
        "console": "integratedTerminal"
    },
    {
        "name": "MD Types",
        "type": "cppvsdbg",
        "request": "launch",
        "program": "${workspaceFolder}\\types\\type_metadata.exe",
        "symbolOptions": {
            "searchPaths": ["${workspaceFolder}\\types"],
            "searchMicrosoftSymbolServer": true
        },
        "args": ["-f", "types", "generated", "engine/camera.type"],
        "stopAtEntry": false,
        "preLaunchTask": "build_types",
        "cwd": "${workspaceFolder}\\types",
        "environment": [],
        "console": "integratedTerminal"
    },
    {
        "name": "Python: Current File",
        "type": "python",
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal",
        "cwd": "${fileDirname}",
    },
    ]
}