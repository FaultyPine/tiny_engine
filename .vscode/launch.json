{
    "configurations": [
    {
        "name": "Testbed",
        "type": "cppvsdbg",
        "request": "launch",
        "program": "${workspaceFolder}\\bin\\Testbed.exe",
        "args": ["${workspaceFolder}\\res\\"],
        "stopAtEntry": false,
        "preLaunchTask": "build",
        "cwd": "${workspaceFolder}\\bin",
        "environment": [],
        "console": "integratedTerminal"
    },
    {
        "name": "Editor",
        "type": "cppvsdbg",
        "request": "launch",
        "program": "${workspaceFolder}\\bin\\TinyEditor.exe",
        "args": ["${workspaceFolder}\\res\\"],
        "stopAtEntry": false,
        "preLaunchTask": "build",
        "cwd": "${workspaceFolder}\\bin",
        "environment": [],
        "console": "integratedTerminal"
    },
    {
        "name": "MD Types",
        "type": "cppvsdbg",
        "request": "launch",
        "program": "${workspaceFolder}\\types\\type_metadata.exe",
        "args": ["types", "generated"],
        "stopAtEntry": false,
        "preLaunchTask": "build_types",
        "cwd": "${workspaceFolder}\\types",
        "environment": [],
        "console": "integratedTerminal"
    },
    {
        "name": "Python: Current File",
        "type": "python",
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal",
        "cwd": "${fileDirname}",
    },
    ]
}